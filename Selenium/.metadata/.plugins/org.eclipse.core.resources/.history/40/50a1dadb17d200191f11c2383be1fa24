package com.org.main;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.ie.InternetExplorerDriver;

public class SeleniumDemo {
	
	public void openBrowser(){
		WebDriver driver;
		System.setProperty("webdriver.chrome.driver", "C:\\Users\\sarfaraz.ahmad\\Desktop\\drivers\\chromedriver.exe");
		 driver =new ChromeDriver();
		
		System.setProperty("webdriver.ie.driver", "C:\\Users\\sarfaraz.ahmad\\Desktop\\drivers\\IEDriverServer.exe");
		 driver = new InternetExplorerDriver();
		
		System.setProperty("webdriver.gecko.driver", "C:\\Users\\sarfaraz.ahmad\\Desktop\\drivers\\geckodriver.exe");
		 driver = new FirefoxDriver();
		
	}

	public static void main(String[] args) {
		
		new SeleniumDemo().openBrowser();
		
		String browserType = "";
		
		

	}

}
